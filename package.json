{
  "name": "luna-bot",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "start": "node src/server.js",
    "build": "npm run clear && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "clear": "rm -rf dist",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/glob": "8.0.0",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "eslint": "8.23.0",
    "eslint-plugin-react": "7.31.8",
    "vitest": "0.23.2",
    "@types/express": "4.17.14",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@discordjs/rest": "1.1.0",
    "@discordjs/voice": "0.11.0",
    "@prisma/client": "4.3.1",
    "axios": "0.27.2",
    "coralogix-logger": "1.1.28",
    "discord.js": "14.3.0",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "glob": "8.0.3",
    "libsodium-wrappers": "0.7.10",
    "nodemon": "2.0.19",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.0",
    "undici": "5.10.0",
    "youtube-sr": "4.3.4",
    "ytdl-core": "4.11.2",
    "ytdl-core-discord": "1.3.1"
  }
}
