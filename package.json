{
  "name": "luna-bot",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --exit-child --respawn --ignore-watch node_modules src/server.ts",
    "start": "node src/server.js",
    "build": "npm run clear && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist/src",
    "clear": "rm -rf dist",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepare": "husky install",
    "lint": "eslint --ext .ts src --fix && prettier --write src/**/*.{ts,js,json,md}"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/glob": "8.0.0",
    "@types/jsonwebtoken": "8.5.9",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "@vitest/coverage-c8": "0.23.4",
    "eslint": "8.23.0",
    "eslint-plugin-react": "7.31.8",
    "prettier": "2.7.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0",
    "tscpaths": "0.0.9",
    "vitest": "0.23.2"
  },
  "dependencies": {
    "husky": "8.0.1",
    "@discordjs/rest": "1.1.0",
    "@discordjs/voice": "0.11.0",
    "@prisma/client": "4.3.1",
    "axios": "0.27.2",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dd-trace": "3.3.1",
    "discord.js": "14.3.0",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "glob": "8.0.3",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "libsodium-wrappers": "0.7.10",
    "nodemon": "2.0.19",
    "ts-node": "10.9.1",
    "undici": "5.10.0",
    "youtube-sr": "4.3.4",
    "ytdl-core": "4.11.2",
    "ytdl-core-discord": "1.3.1"
  }
}
